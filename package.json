  "apidoc": {
    "title": "Custom apiDoc browser title",
    "version": "1.0.0",
    "url": "http://sensorthings.geosas.fr/v1.0",
    "proxy": "http://sensorthings.geosas.fr",
    "withGenerator": false,
    "header": {
      "title": "Introduction",
      "filename": "header.md"
    },
    "footer": {
      "title": "Errors",
      "filename": "footer.md"
    },
    "order": [
      "Root",
      "Things",
      "Locations",
      "HistoricalLocations",
      "Datastreams",
      "Sensors",
      "ObservedProperties",
      "Observations",
      "CreateObservations",
      "FeaturesOfInterest",
      "Query",
      "Format"
    ]
  }
